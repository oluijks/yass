{
  "name": "yass",
  "type": "module",
  "version": "1.0.0",
  "description": "Yet another sveltekit starter",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "flyctl deploy",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "sys:info": "npx envinfo --system --binaries --browsers --npmPackages \"{svelte,@sveltejs/*,vite}\"",
    "test": "npm run test:unit -- --run",
    "test:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "debug:dev": "node --inspect node_modules/@sveltejs/kit/dist/cli.js dev",
    "debug:start": "node --inspect node_modules/@sveltejs/kit/dist/cli.js start",
    "docker:build": "docker build -t yass .",
    "docker:run": "docker run --env-file .env -e ORIGIN=http://localhost:3000 -p 3000:3000 --name yass yass",
    "docker:stop": "docker stop yass",
    "docker:remove": "docker rm yass",
    "docker:update": "pnpm run docker:stop && pnpm run docker:remove && pnpm run docker:build && pnpm run docker:run"
  },
  "dependencies": {
    "@node-rs/argon2": "^2.0.2",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.16",
    "better-sqlite3": "11.8.0",
    "drizzle-orm": "^0.38.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.14.0",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/enhanced-img": "^0.4.4",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.6",
    "@types/better-sqlite3": "^7.6.12",
    "@vitest/coverage-v8": "3.0.2",
    "@vitest/ui": "^3.0.2",
    "autoprefixer": "^10.4.20",
    "bits-ui": "1.0.0-next.72",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.1",
    "embla-carousel-svelte": "^8.5.2",
    "eslint": "^9.18.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-svelte": "^2.46.1",
    "formsnap": "2.0.0",
    "jsdom": "^26.0.0",
    "lucide-svelte": "^0.473.0",
    "mode-watcher": "^0.5.0",
    "phosphor-svelte": "^3.0.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "svelte": "^5.19.0",
    "svelte-check": "^4.1.4",
    "svelte-eslint-parser": "^0.43.0",
    "svelte-sonner": "^0.3.28",
    "sveltekit-superforms": "^2.22.1",
    "tailwind-merge": "^2.6.0",
    "tailwind-variants": "^0.3.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.3",
    "typescript-svelte-plugin": "^0.3.45",
    "vite": "^6.0.7",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^3.0.2",
    "zod": "^3.24.1"
  }
}
